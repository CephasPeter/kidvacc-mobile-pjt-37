<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/back"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_25sdp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="@dimen/_20sdp"
        android:src="@drawable/ic_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/shapeableImageView2"
        app:tint="#08517A" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/shapeableImageView2"
        android:layout_width="@dimen/_55sdp"
        android:layout_height="@dimen/_55sdp"
        android:layout_marginTop="@dimen/_15sdp"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout9"
        app:layout_constraintStart_toEndOf="@+id/back"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.App.CornerSize50Percent"
        app:srcCompat="@drawable/dr_patty_black_woman_entrepreneur_1" />

    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_20sdp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/shapeableImageView2">

        <ImageView
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_gravity="center_horizontal"
            android:scaleX=".7"
            android:scaleY=".7"
            android:src="@drawable/ic_vector__6_" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:text="Information"
        android:textColor="#000000"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shapeableImageView2" />

    <TextView
        android:id="@+id/first_name_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="First Name"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textStyle="bold"
        android:textColor="#08517A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/first_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="First Name"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/first_name_header" />

    <TextView
        android:id="@+id/phone_number_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="Phone Number"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textStyle="bold"
        android:textColor="#08517A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/first_name" />

    <TextView
        android:id="@+id/phone_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="+2348012345678"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_number_header" />

    <TextView
        android:id="@+id/last_name_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_60sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="Last Name"
        android:textColor="#08517A"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/first_name_header"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/last_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="Williams"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        app:layout_constraintStart_toStartOf="@id/last_name_header"
        app:layout_constraintTop_toBottomOf="@+id/first_name_header" />

    <TextView
        android:id="@+id/email_address_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="Email Address"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textStyle="bold"
        android:textColor="#08517A"
        app:layout_constraintStart_toStartOf="@id/last_name_header"
        app:layout_constraintTop_toBottomOf="@+id/first_name" />

    <TextView
        android:id="@+id/email_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="tolaniwilliams@gmail.com"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        app:layout_constraintStart_toStartOf="@id/last_name_header"
        app:layout_constraintTop_toBottomOf="@+id/phone_number_header" />

    <TextView
        android:id="@+id/address_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="Address"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textStyle="bold"
        android:textColor="#08517A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_number" />

    <TextView
        android:id="@+id/address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:text="10B, Utopia Street, Agege, Lagos State"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_header" />

    <TextView
        android:id="@+id/dashboard_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_40sdp"
        android:text="Dashboard"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textStyle="bold"
        android:textColor="#08517A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toBottomOf="@+id/dashboard_header"
        app:layout_constrainedHeight="true"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_5sdp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_1sdp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="@dimen/_110sdp"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="#FCFCFC"
                app:cardCornerRadius="@dimen/_8sdp"
                app:strokeColor="#DA8F06"
                app:strokeWidth="@dimen/_1sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_10sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_85sdp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:orientation="vertical"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Registered"
                            android:textColor="#08517A"
                            android:gravity="center_vertical"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Children"
                            android:textColor="#08517A"
                            android:gravity="center_vertical"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="@dimen/_10sdp"
                            >

                            <ImageView
                                android:id="@+id/three"
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_25sdp"
                                android:src="@drawable/ic__three"
                                android:layout_marginEnd="@dimen/_5sdp"/>

                            <TextView
                                android:id="@+id/top"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Complete"
                                android:textColor="#08517A"
                                android:gravity="center_vertical"
                                android:layout_toEndOf="@id/three"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Registrations"
                                android:textColor="#08517A"
                                android:gravity="center_vertical"
                                android:layout_toEndOf="@id/three"
                                android:layout_below="@id/top"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        </RelativeLayout>

                    </LinearLayout>

                </RelativeLayout>

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="@dimen/_150sdp"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="#FCFCFC"
                app:cardCornerRadius="@dimen/_8sdp"
                app:strokeColor="#DA8F06"
                app:strokeWidth="@dimen/_1sdp"
                >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_width="@dimen/_110sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:orientation="vertical"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Parent Score"
                            android:textColor="#08517A"
                            android:gravity="center_vertical"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="@dimen/_10sdp"
                            >

                            <ImageView
                                android:id="@+id/bplus"
                                android:layout_width="@dimen/_25sdp"
                                android:layout_height="@dimen/_25sdp"
                                android:src="@drawable/ic_b_plus"
                                android:layout_marginEnd="@dimen/_5sdp"/>

                            <TextView
                                android:id="@+id/top1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Complete"
                                android:textColor="#08517A"
                                android:gravity="center_vertical"
                                android:layout_toEndOf="@id/bplus"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Registrations"
                                android:textColor="#08517A"
                                android:gravity="center_vertical"
                                android:layout_toEndOf="@id/bplus"
                                android:layout_below="@id/top1"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        </RelativeLayout>

                    </LinearLayout>

                </RelativeLayout>

            </com.google.android.material.card.MaterialCardView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginEnd="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_1sdp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="#FCFCFC"
                app:cardCornerRadius="@dimen/_8sdp"
                app:strokeColor="#DA8F06"
                app:strokeWidth="@dimen/_1sdp"
                android:layout_marginTop="@dimen/_10sdp"
                >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_85sdp">

                    <LinearLayout
                        android:layout_width="@dimen/_170sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:orientation="vertical"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Appointment"
                            android:textColor="#08517A"
                            android:gravity="center_vertical"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Records"
                            android:textColor="#08517A"
                            android:gravity="center_vertical"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="@dimen/_10sdp"
                            >

                            <ImageView
                                android:id="@+id/nine"
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_25sdp"
                                android:src="@drawable/ic__three"
                                android:layout_marginEnd="@dimen/_5sdp"/>

                            <TextView
                                android:id="@+id/top2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Appointments"
                                android:textColor="#08517A"
                                android:gravity="center_vertical"
                                android:layout_toEndOf="@id/nine"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Attended"
                                android:textColor="#08517A"
                                android:gravity="center_vertical"
                                android:layout_toEndOf="@id/nine"
                                android:layout_below="@id/top2"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        </RelativeLayout>

                    </LinearLayout>

                </RelativeLayout>

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/settings"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="#FCFCFC"
                app:cardCornerRadius="@dimen/_8sdp"
                app:strokeColor="#DA8F06"
                app:strokeWidth="@dimen/_1sdp"
                android:layout_marginTop="@dimen/_10sdp"
                >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_width="@dimen/_170sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:orientation="vertical"
                        android:gravity="center_vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Settings &amp; Support"
                            android:textColor="#08517A"
                            android:gravity="center_vertical"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="@dimen/_10sdp"
                            >

                            <ImageView
                                android:id="@+id/settings1"
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_25sdp"
                                android:src="@drawable/ic_vector__10_"
                                android:layout_marginEnd="@dimen/_5sdp"/>

                            <TextView
                                android:id="@+id/top3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Check Various Support"
                                android:textColor="#08517A"
                                android:gravity="center_vertical"
                                android:layout_toEndOf="@id/settings1"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="And Contact Settings"
                                android:textColor="#08517A"
                                android:gravity="center_vertical"
                                android:layout_toEndOf="@id/settings1"
                                android:layout_below="@id/top3"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        </RelativeLayout>

                    </LinearLayout>

                </RelativeLayout>

            </com.google.android.material.card.MaterialCardView>

        </LinearLayout>

    </LinearLayout>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/add_a_child"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_50sdp"
        android:layout_marginEnd="@dimen/_50sdp"
        android:backgroundTint="#08517A"
        android:text="@string/add_a_child"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@id/sign_out"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/sign_out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/Widget.MaterialComponents.Button.TextButton"
        android:layout_marginStart="@dimen/_50sdp"
        android:layout_marginEnd="@dimen/_50sdp"
        android:text="Sign Out"
        android:textColor="#08517A"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <FrameLayout
        android:id="@+id/home_frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:elevation="@dimen/_5sdp"
        android:outlineProvider="none"
        />

</androidx.constraintlayout.widget.ConstraintLayout>